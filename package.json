{
  "name": "nuxt-upload-kit",
  "version": "0.1.23",
  "description": "A powerful, plugin-based file upload manager for Nuxt applications",
  "license": "MIT",
  "repository": "https://github.com/genu/nuxt-upload-kit.git",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./providers/s3": {
      "types": "./dist/providers/s3.d.mts",
      "import": "./dist/providers/s3.mjs"
    },
    "./providers/azure-datalake": {
      "types": "./dist/providers/azure-datalake.d.mts",
      "import": "./dist/providers/azure-datalake.mjs"
    },
    "./providers/firebase": {
      "types": "./dist/providers/firebase.d.mts",
      "import": "./dist/providers/firebase.mjs"
    }
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "pnpm dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "docs:dev": "nuxt dev docs --extends docus"
  },
  "dependencies": {
    "mitt": "^3.0.1"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "3.986.0",
    "@aws-sdk/lib-storage": "3.986.0",
    "@azure/storage-file-datalake": "^12.29.0",
    "@ffmpeg/ffmpeg": "0.12.15",
    "@ffmpeg/util": "0.12.2",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.13.0",
    "@nuxt/kit": "4.3.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "4.3.1",
    "@nuxt/test-utils": "^4.0.0",
    "@types/node": "latest",
    "@vitejs/plugin-vue": "6.0.4",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "firebase": "12.9.0",
    "happy-dom": "20.5.3",
    "nuxt": "4.3.1",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "unbuild": "3.6.1",
    "vitest": "4.0.18",
    "vue": "3.5.28",
    "vue-tsc": "3.2.4"
  },
  "peerDependencies": {
    "@azure/storage-file-datalake": "^12.0.0",
    "@ffmpeg/ffmpeg": "^0.12.0",
    "@ffmpeg/util": "^0.12.0",
    "firebase": "^10.0.0 || ^11.0.0 || ^12.0.0"
  },
  "peerDependenciesMeta": {
    "@azure/storage-file-datalake": {
      "optional": true
    },
    "@ffmpeg/ffmpeg": {
      "optional": true
    },
    "@ffmpeg/util": {
      "optional": true
    },
    "firebase": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.29.2"
}
