{
  "name": "nuxt-upload-kit",
  "version": "0.1.7",
  "description": "A powerful, plugin-based file upload manager for Nuxt applications",
  "license": "MIT",
  "repository": "https://github.com/genu/nuxt-upload-kit.git",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "pnpm dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "release": "pnpm lint && pnpm test && pnpm prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "docs:dev": "nuxt dev docs --extends docus"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2",
    "mitt": "^3.0.1"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.969.0",
    "@aws-sdk/lib-storage": "^3.969.0",
    "@azure/storage-file-datalake": "^12.28.1",
    "@ffmpeg/ffmpeg": "0.12.15",
    "@ffmpeg/util": "0.12.2",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.23.0",
    "@types/node": "latest",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.17",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "firebase": "^12.7.0",
    "happy-dom": "^20.2.0",
    "nuxt": "^4.2.2",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "vitest": "^4.0.17",
    "vue": "^3.5.26",
    "vue-tsc": "^3.2.2"
  },
  "peerDependencies": {
    "@azure/storage-file-datalake": "^12.0.0",
    "@ffmpeg/ffmpeg": "^0.12.0",
    "@ffmpeg/util": "^0.12.0"
  },
  "peerDependenciesMeta": {
    "@azure/storage-file-datalake": {
      "optional": true
    },
    "@ffmpeg/ffmpeg": {
      "optional": true
    },
    "@ffmpeg/util": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.26.2"
}
